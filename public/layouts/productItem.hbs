{{#if all}}
 <thead class="thead-dark">
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody>
  {{#each all}}
  <tr>
      <td>{{this.title}}</td>
      <td>{{this.price}}</td>
      <td><img src="{{this.thumbnail}}" alt="{{this.title}}" /></td>
  </tr>
    {{/each}}
{{else}}
  <tr>
    <td><h4>No hay productos para mostrar</h4></td>
  </tr>
{{/if}}
</tbody>